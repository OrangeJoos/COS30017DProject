<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GenerateName">

    <TextView
        android:id="@+id/testGender"
        android:layout_width="144dp"
        android:layout_height="35dp"
        android:layout_marginTop="70dp"
        android:text="TextView"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/testStartYear"
        android:layout_width="142dp"
        android:layout_height="39dp"
        android:layout_marginTop="46dp"
        android:text="TextView"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/testGender" />

    <TextView
        android:id="@+id/testEndYear"
        android:layout_width="147dp"
        android:layout_height="51dp"
        android:layout_marginStart="134dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="130dp"
        android:text="TextView"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/testStartYear" />
</androidx.constraintlayout.widget.ConstraintLayout>